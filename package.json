{
  "name": "fashionista-portal",
  "version": "1.0.0",
  "description": "Fashion event management portal for Fashionistas",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "dev": "nodemon -r tsconfig-paths/register --exec ts-node src/server.ts",
    "dev:clean": "ts-node src/utils/check-port.ts && npm run dev",
    "build": "tsc",
    "migrate": "ts-node scripts/migrate.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:setup": "ts-node scripts/start-test-env.ts",
    "test:teardown": "docker-compose down -v",
    "test:ci": "npm run test:setup && npm test && npm run test:teardown",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "predev": "ts-node src/utils/check-port.ts"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.1",
    "@supabase/supabase-js": "^2.48.1",
    "@types/uuid": "^10.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "framer-motion": "^12.3.0",
    "helmet": "^7.1.0",
    "lucide-react": "^0.474.0",
    "react-router-dom": "^7.1.5",
    "supabase-cli": "^0.0.21",
    "tailwind-merge": "^3.0.1",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
